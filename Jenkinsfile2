pipeline {
	
	
  	agent { label 'linux' }
	
	options {
		buildDiscarder(logRotator(numToKeepStr:'5'))
	}
  
	environment {
		DOCKERHUB_CREDENTIALS = credentials('credential-docker')
	}
  
  stages {
	  
    stage('Checkout Code') {
      steps {
        git(url: 'https://github.com/akkaoui-abdou/fastapi-action.git', branch: 'main')
      }
    }

    stage('Log git') {
      steps {
        sh 'git log'
      }
    }
    
  }
  
}

